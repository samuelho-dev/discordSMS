  "bugs": {
    "url": "https://github.com/samuelho-dev/DiscordSMS/issues"
  },
  "homepage": "https://github.com/samuelho-dev/DiscordSMS#readme",
  "dependencies": {
    "discord.js": "^14.11.0",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "express-slow-down": "^1.6.0",
    "knex": "^2.4.2",
    "node-fetch": "^3.3.1",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "twilio": "^4.11.2",
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.1",
